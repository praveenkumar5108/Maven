package test;

public class Employee {
	
	public int EmpId;
	public int Salary;
	
	String department;
	
  
	public Employee() {
		System.out.println("INSIDE EMPLUEE");
	}
	
	
	public void display() {
		System.out.println("Employee id is:" + " " + EmpId);
		System.out.println("Employee salary is :"  + " " + Salary);
	}

}
